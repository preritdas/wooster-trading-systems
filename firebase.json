{
  "hosting": [
    {
      "site": "wooster-trading-systems",
      "public": "results",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**",
        "**/optimizers/**",
        "**/raw/**",
        "**/oldindex.html"
      ],
      "headers": [
        {
          "source": "**",
          "headers": [
            {
              "key": "Cache-Control",
              "value": "no-cache"
            }
          ]
        }
      ]
    },
    {
      "site": "wooster-docs",
      "public": "docs/build/html",
      "ignore": [
        "firebase.json",
        "**/.*"
      ],
      "headers": [
        {
          "source": "**",
          "headers": [
            {
              "key": "Cache-Control",
              "value": "no-cache"
            }
          ]
        }
      ]
    }
  ]
}
